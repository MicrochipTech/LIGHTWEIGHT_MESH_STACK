<asf xmlversion="1.0">
	<select-by-device id="common.components.wifi.winc1500.power_save_mode_example">
		<module type="application" id="common.components.wifi.winc1500.power_save_mode_example#0" caption="WINC1500 Power Save Mode Example">
			<!-- Summary Description -->
			<info type="description" value="summary">
				This example demonstrates the use of the WINC1500 with the SAMD21 Xplained Pro board to set the PS (Power Save) mode.
			</info>

			<!-- Source -->
			<build type="include-path" value="."/>
			<build type="c-source" value="main21.c"/>
			<build type="header-file" value="main.h"/>

			<!-- Device support -->
			<device-support value="samd21"/>
			<device-support value="saml21"/>

			<!-- Required modules -->
			<require idref="common.components.wifi.winc1500"/>
			<require idref="sam0.drivers.tcc"/>
			<require idref="sam0.utils"/>
			<require idref="sam0.utils.stdio"/>
			<require idref="sam0.utils.stdio.stdio_serial"/>
			<info type="gui-flag" value="move-to-root"/>

			<build type="define"   name="CONF_PERIPH" value=""/>
		</module>
	</select-by-device>
</asf>
