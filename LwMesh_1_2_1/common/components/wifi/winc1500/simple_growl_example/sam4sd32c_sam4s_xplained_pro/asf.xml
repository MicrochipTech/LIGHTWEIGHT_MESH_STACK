<asf xmlversion="1.0">
	<project caption="WINC1500 Simple Growl Example for SAM4S XPlained Pro" id="common.components.wifi.winc1500.simple_growl_example.sam4s_xplained_pro">
		<!-- Summary Description -->
		<info type="description" value="summary">
			This example transmits a notification from the WINC1500 Wi-Fi module (based on a certain trigger) to a cloud based service which in turn sends back a notification to a specific phone application.
		</info>

		<!-- Source -->
		<build type="include-path" value="."/>
		<build type="header-file" value="conf_board.h"/>
		<build type="header-file" value="conf_clock.h"/>
		<build type="header-file" value="conf_uart_serial.h"/>
		<build type="header-file" value="conf_winc.h"/>

		<!-- Device support -->
		<device-support value="atsam4sd32c"/>

		<!-- Required modules -->
		<require idref="common.components.wifi.winc1500.simple_growl_example"/>
		<require idref="board.sam4s_xplained_pro"/>

		<!-- Generators -->
		<generator value="armgcc"/>
		<generator value="as5_arm"/>
		<generator value="doxygen"/>
		<config name="config.compiler.armgcc.create_aux" value="no"/>
		<config name="config.compiler.as5_arm.create_aux" value="no"/>
	</project>
</asf>
