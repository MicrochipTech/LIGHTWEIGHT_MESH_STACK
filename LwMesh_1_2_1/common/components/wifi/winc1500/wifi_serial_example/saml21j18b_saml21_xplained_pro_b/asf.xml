<asf xmlversion="1.0">
	<project caption="WINC1500 Wi-Fi Serial Example for SAML21 XPlained Pro B" id="common.components.wifi.winc1500.wifi_serial_example.saml21_xplained_pro_b">
		<!-- Summary Description -->
		<info type="description" value="summary">
			This example demonstrates the use of the WINC1500 with the SAML21 Xplained Pro board to emulate a serial port between two devices. It reads input data from serial interface and sends it via Wi-Fi connection. The EDBG interface will print out messages you typed or received, hence simulating a chat application.
		</info>

		<!-- Source -->
		<build type="include-path" value="."/>
		<build type="header-file" value="conf_board.h"/>
		<build type="header-file" value="conf_clocks.h"/>
		<build type="header-file" value="conf_extint.h"/>
		<build type="header-file" value="conf_spi.h"/>
		<build type="header-file" value="conf_winc.h"/>

		<!-- Device support -->
		<device-support value="atsaml21j18b"/>

		<!-- Required modules -->
		<require idref="common.components.wifi.winc1500.wifi_serial_example"/>
		<require idref="board.saml21_xplained_pro_b"/>

		<!-- Generators -->
		<generator value="armgcc"/>
		<generator value="as5_arm"/>
		<generator value="doxygen"/>
		<config name="config.compiler.armgcc.create_aux" value="no"/>
		<config name="config.compiler.as5_arm.create_aux" value="no"/>
	</project>
</asf>
