		<build type="header-file" value="conf_sw_timer.h"/>		
		<build type="header-file" value="conf_winc.h"/>
		<build type="header-file" value="conf_sleepmgr.h"/>

		<!-- Device support -->
		<device-support value="atsamg55j19"/>

		<!-- Required modules -->
