<asf xmlversion="1.0">
	<project caption="WINC1500 Locate IP Address Example for SAML22 XPlained Pro" id="common.components.wifi.winc1500.locate_ip_address_example.saml22_xplained_pro_b">
		<!-- Summary Description -->
		<info type="description" value="summary">
			This example demonstrates the use of the WINC1500 with the SAML21 Xplained Pro board to get GPS position from the network provider IP address using a thirdparty HTTP server.
		</info>

		<!-- Source -->
		<build type="include-path" value="."/>
		<build type="header-file" value="conf_board.h"/>
		<build type="header-file" value="conf_clocks.h"/>
		<build type="header-file" value="conf_extint.h"/>
		<build type="header-file" value="conf_spi.h"/>
		<build type="header-file" value="conf_sw_timer.h"/>
		<build type="header-file" value="conf_winc.h"/>

		<!-- Device support -->
		<device-support value="atsaml22n18a"/>

		<!-- Required modules -->
		<require idref="common.components.wifi.winc1500.locate_ip_address_example"/>
		<require idref="board.saml22_xplained_pro_b"/>

		<!-- Generators -->
		<generator value="armgcc"/>
		<generator value="as5_arm"/>
		<generator value="doxygen"/>
		<config name="config.compiler.armgcc.create_aux" value="no"/>
		<config name="config.compiler.as5_arm.create_aux" value="no"/>
	</project>
</asf>
