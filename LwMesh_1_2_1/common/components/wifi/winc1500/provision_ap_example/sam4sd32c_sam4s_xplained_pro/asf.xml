<asf xmlversion="1.0">
	<project caption="WINC1500 Wi-Fi Provisioning Example for SAM4S XPlained Pro" id="common.components.wifi.winc1500.provision_ap_example.sam4s_xplained_pro">
		<!-- Summary Description -->
		<info type="description" value="summary">
			This example demonstrates one way of provisioning the WINC1500. It starts in Access Point (AP) mode with open security so an Android smart phone with the supplied application can send provisioning information to the AP via TCP connection. Finally, the WINC1500 connects to the desired AP.
		</info>

		<!-- Source -->
		<build type="include-path" value="."/>
		<build type="header-file" value="conf_board.h"/>
		<build type="header-file" value="conf_clock.h"/>
		<build type="header-file" value="conf_uart_serial.h"/>
		<build type="header-file" value="conf_winc.h"/>

		<!-- Device support -->
		<device-support value="atsam4sd32c"/>

		<!-- Required modules -->
		<require idref="common.components.wifi.winc1500.provision_ap_example"/>
		<require idref="board.sam4s_xplained_pro"/>

		<!-- Generators -->
		<generator value="armgcc"/>
		<generator value="as5_arm"/>
		<generator value="doxygen"/>
		<config name="config.compiler.armgcc.create_aux" value="no"/>
		<config name="config.compiler.as5_arm.create_aux" value="no"/>
	</project>
</asf>
