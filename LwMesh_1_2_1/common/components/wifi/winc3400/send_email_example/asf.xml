<asf xmlversion="1.0">
	<select-by-device id="common.components.wifi.winc3400.send_email_example">
		<module type="application" id="common.components.wifi.winc3400.send_email_example#0" caption="WINC3400 Send Email Example">
			<!-- Summary Description -->
			<info type="description" value="summary">
				This example demonstrates how to implement a send email client via SMTP using the WINC3400 Wi-Fi module.
			</info>

			<!-- Source -->
			<build type="include-path" value="."/>
			<build type="c-source" value="base64.c"/>
			<build type="c-source" value="main21.c"/>
			<build type="header-file" value="main.h"/>

			<!-- Device support -->
			<device-support value="samd21"/>

			<!-- Required modules -->
			<require idref="common.components.wifi.winc3400"/>
			<require idref="sam0.utils"/>
			<require idref="sam0.utils.stdio"/>
			<require idref="sam0.utils.stdio.stdio_serial"/>
			<info type="gui-flag" value="move-to-root"/>
		</module>
	</select-by-device>
</asf>
