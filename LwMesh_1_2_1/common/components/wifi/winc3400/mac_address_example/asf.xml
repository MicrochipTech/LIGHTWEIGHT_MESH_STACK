<asf xmlversion="1.0">
	<select-by-device id="common.components.wifi.winc3400.mac_address_example">
		<module type="application" id="common.components.wifi.winc3400.mac_address_example#0" caption="WINC3400 MAC Address Example">
			<!-- Summary Description -->
			<info type="description" value="summary">
				This example demonstrates how to retrieve the MAC address of the WINC3400 Wi-Fi module.
			</info>

			<!-- Source -->
			<build type="include-path" value="."/>
			<build type="c-source" value="main21.c"/>

			<!-- Device support -->
			<device-support value="samd21"/>

			<!-- Required modules -->
			<require idref="common.components.wifi.winc3400"/>
			<require idref="sam0.utils"/>
			<require idref="sam0.utils.stdio"/>
			<require idref="sam0.utils.stdio.stdio_serial"/>
			<info type="gui-flag" value="move-to-root"/>
		</module>
		<module type="application" id="common.components.wifi.winc3400.mac_address_example#1" caption="WINC3400 MAC Address Example">
			<!-- Summary Description -->
			<info type="description" value="summary">
				This example demonstrates how to retrieve the MAC address of the WINC3400 Wi-Fi module.
			</info>

			<!-- Source -->
			<build type="include-path" value="."/>
			<build type="c-source" value="main.c"/>

			<!-- Device support -->
			<device-support value="sam4s"/>
			<device-support value="samg55"/>

			<!-- Required modules -->
			<require idref="common.components.wifi.winc3400"/>
			<require idref="common.services.basic.clock"/>
			<require idref="common.utils.stdio.stdio_serial"/>
			<info type="gui-flag" value="move-to-root"/>
		</module>
	</select-by-device>
</asf>
