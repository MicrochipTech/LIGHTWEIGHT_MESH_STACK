<asf xmlversion="1.0">
	<module type="component" id="common.components.wifi.winc3400.ble.ble_profiles.hr_sensor" caption="BLE-Heart Rate Sensor Profile">
		<info type="description" value="summary">
			Heart Rate Profile for WINC3400. This Profile sends Heart Rate information over BLE
		</info>
		<build type="include-path"  value="./"/>
  		<build type="header-file"   value="hr_sensor.h"/>
  		<build type="c-source"      value="hr_sensor.c"/>
  		<require idref="common.components.wifi.winc3400.ble.ble_services.heart_rate"/>

		<device-support value="samd21"/>
		<device-support value="sam4s"/>
		<device-support value="samg55"/>
		<device-support value="same70"/>

	</module>

</asf>
