				<keyword value="USB"/>
				<keyword value="Communication"/>
			</info>
			<info type="gui-flag" value="move-to-root"/>
		</module>
		<module type="application" id="common.services.usb.class.hid.device.generic.example#1" caption="USB Device HID Generic Example">
