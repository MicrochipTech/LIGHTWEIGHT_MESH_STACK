<asf xmlversion="1.0">
	<project caption="USB Dual Role (Device and Host) HID mouse for SAME70-Xplained" id="common.services.usb.class.hid.dual.mouse.example.same70_xplained">
		<build type="header-file" value="conf_board.h"/>
		<build type="header-file" value="conf_clock.h"/>
		<build type="header-file" value="conf_sleepmgr.h"/>
		<build type="c-source" value="ui.c"/>
		<build type="include-path" value="."/>
		<config name="config.common.services.usb.class.device" value="hid_mouse"/>
		<config name="config.common.services.usb.class.host" value="hid_mouse"/>
		<require idref="common.services.usb.class.hid.dual.mouse.example"/>
		<require idref="board.same70_xplained"/>
		<config name="config.board.same70_xplained.led" value="yes"/>
		<config name="config.compiler.armgcc.create_aux" value="no"/>
		<device-support value="atsame70q21b"/>
	</project>
</asf>
