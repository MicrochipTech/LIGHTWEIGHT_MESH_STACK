		<module type="meta" id="config.board.stk600-rc100x.init#yes" caption="STK600-RC100X initialization support enabled">
			<build type="c-source" value="init.c"/>
			<require idref="common.services.basic.gpio"/>
			<require idref="xmega.drivers.ioport"/>
		</module>
	</select-by-config>

