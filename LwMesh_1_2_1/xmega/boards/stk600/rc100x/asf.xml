<asf xmlversion="1.0">
	<select-by-config id="config.board.stk600-rc100x.led" name="config.board.stk600-rc100x.led" caption="STK600-RC100X LED support" default="yes">
		<module type="meta" id="config.board.stk600-rc100x.led#no" caption="STK600-RC100X LED support disabled">
		</module>
		<module type="meta" id="config.board.stk600-rc100x.led#yes" caption="STK600-RC100X LED support enabled">
			<build type="header-file" subtype="api" value="led.h"/>
			<build type="include-path" value="."/>
			<require idref="common.services.basic.gpio"/>
			<require idref="xmega.drivers.ioport"/>
		</module>
	</select-by-config>

	<select-by-config id="config.board.stk600-rc100x.init" name="config.board.stk600-rc100x.init" caption="STK600-RC100X initialization support" default="yes">
