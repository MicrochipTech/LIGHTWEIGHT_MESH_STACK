<asf xmlversion="1.0">
	<project caption="TWI EEPROM Example for SAM4N-Xplained-Pro" id="sam.drivers.twi.twi_eeprom_example.sam4n_xplained_pro">
		<info type="description" value="summary">
		The application demonstrates how to use the SAM TWI driver to access an external serial EEPROM chip.
		</info>
		<build type="header-file" value="conf_board.h"/>
		<build type="header-file" value="conf_clock.h"/>
		<build type="header-file" value="conf_uart_serial.h"/>
		<build type="include-path" value="."/>
		<require idref="sam.drivers.twi.twi_eeprom_example"/>
		<require idref="board.sam4n_xplained_pro"/>
		<config name="config.board.sam4n_xplained_pro.led" value="yes"/>
		<generator value="iarewarm"/>
		<generator value="armgcc"/>
		<generator value="as5_arm"/>
		<config name="config.compiler.armgcc.create_aux" value="no"/>
		<generator value="doxygen"/>
		<device-support value="atsam4n16c"/>
	</project>
</asf>
