<asf xmlversion="1.0">
	<project caption="CMSIS NVIC example for Arduino Due/X" id="sam.utils.cmsis.cm3_nvic_example.arduino_due_x">
		<info type="description" value="summary">
		The demonstration program makes two buttons as the NVIC trigger. Each button would trigger one LED flash. When an NVIC interrupt preemptive happened, both the LEDs are on. 
		</info>
		<build type="include-path" value="."/>
		<build type="header-file" value="conf_board.h"/>
		<build type="header-file" value="conf_clock.h"/>
		<build type="header-file" value="conf_uart_serial.h"/>
		<build type="include-path" value="."/>
		<require idref="sam.utils.cmsis.cm3_nvic_example"/>
		<require idref="board.arduino_due_x"/>
		<generator value="iarewarm"/>
		<generator value="armgcc"/>
		<generator value="as5_arm"/>
		<generator value="doxygen"/>
		<device-support value="atsam3x8e"/>
	</project>
</asf>
