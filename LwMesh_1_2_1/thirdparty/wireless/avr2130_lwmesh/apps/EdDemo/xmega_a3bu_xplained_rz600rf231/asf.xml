<asf xmlversion="1.0">
	<project caption="LWMesh-EDDemo Application AT86RF231 - AtXmega256A3BU Xplained" id="thirdparty.wireless.avr2130_lwmesh.apps.eddemo.atxmega256a3bu_xplained_rf231" force-caption="true" force-description="true">
