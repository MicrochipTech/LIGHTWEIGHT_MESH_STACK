<asf xmlversion="1.0">
	<select-by-device id="common.components.wifi.winc1500.host_firmware_upgrade.bootloader">
		<module type="application" id="common.components.wifi.winc1500.host_firmware_upgrade.bootloader#1" caption="Bootloader Example for HOST OTA Firmware Upgrade">
			<!-- Summary Description -->
			<info type="description" value="summary">
				This is the Bootloader example for firmware upgrade for the Host MCU through WINC Wi-Fi module.
			</info>

			<!-- Source -->
			<build type="include-path" value="."/>
			<build type="c-source" value="main.c"/>

			<!-- Device support -->
			<device-support value="samg55"/>

			<!-- Required modules -->
			<require idref="common.components.wifi.winc1500.host_firmware_upgrade"/>
			<require idref="sam.drivers.rstc"/>
			<require idref="common.services.basic.clock"/>
			<require idref="common.utils.stdio.stdio_serial"/>
			<info type="gui-flag" value="move-to-root"/>
		</module>
	</select-by-device>
</asf>
