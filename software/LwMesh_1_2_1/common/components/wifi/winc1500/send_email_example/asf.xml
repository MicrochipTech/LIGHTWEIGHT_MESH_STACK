<asf xmlversion="1.0">
	<select-by-device id="common.components.wifi.winc1500.send_email_example">
		<module type="application" id="common.components.wifi.winc1500.send_email_example#0" caption="WINC1500 Send Email Example">
			<!-- Summary Description -->
			<info type="description" value="summary">
				This example demonstrates how to implement a send email client via SMTP using the WINC1500 Wi-Fi module.
			</info>

			<!-- Source -->
			<build type="include-path" value="."/>
			<build type="c-source" value="base64.c"/>
			<build type="c-source" value="main21.c"/>
			<build type="header-file" value="main.h"/>

			<!-- Device support -->
			<device-support value="samd21"/>
			<device-support value="saml21"/>

			<!-- Required modules -->
			<require idref="common.components.wifi.winc1500"/>
			<require idref="sam0.utils"/>
			<require idref="sam0.utils.stdio"/>
			<require idref="sam0.utils.stdio.stdio_serial"/>
			<info type="gui-flag" value="move-to-root"/>
		</module>
		<module type="application" id="common.components.wifi.winc1500.send_email_example#1" caption="WINC1500 Send Email Example">
			<!-- Summary Description -->
			<info type="description" value="summary">
				This example demonstrates how to implement a send email client via SMTP using the WINC1500 Wi-Fi module.
			</info>

			<!-- Source -->
			<build type="include-path" value="."/>
			<build type="c-source" value="base64.c"/>
			<build type="c-source" value="main.c"/>
			<build type="header-file" value="main.h"/>

			<!-- Device support -->
			<device-support value="sam4s"/>
			<device-support value="samg53"/>
			<device-support value="samg55"/>

			<!-- Required modules -->
			<require idref="common.components.wifi.winc1500"/>
			<require idref="common.services.basic.clock"/>
			<require idref="common.utils.stdio.stdio_serial"/>
			<info type="gui-flag" value="move-to-root"/>
		</module>
	</select-by-device>
</asf>
