<asf xmlversion="1.0">
	<select-by-device id="common.components.wifi.winc1500.iperf_server_example">
		<module type="application" id="common.components.wifi.winc1500.iperf_server_example#0" caption="WINC1500 Iperf Example">
			<!-- Summary Description -->
			<info type="description" value="summary">
				This example demonstrates iperf using the WINC1500 Wi-Fi module.
			</info>

			<!-- Source -->
			<build type="include-path" value="."/>
			<build type="c-source" value="iperf.c"/>
			<build type="c-source" value="main21.c"/>
			<build type="header-file" value="iperf.h"/>
			<build type="header-file" value="bsp_app/include/nm_bsp_samd21_app.h"/>
			<build type="c-source" value="bsp_app/source/nm_bsp_samd21_app.c"/>


			<!-- Device support -->
			<device-support value="samd21"/>

			<!-- Required modules -->
			<require idref="common.components.wifi.winc1500"/>
			<require idref="sam0.drivers.tcc"/>
			<require idref="sam0.drivers.dma"/>
			<require idref="sam0.utils"/>
			<require idref="sam0.utils.stdio"/>
			<require idref="sam0.utils.stdio.stdio_serial"/>
			<info type="gui-flag" value="move-to-root"/>
		</module>
