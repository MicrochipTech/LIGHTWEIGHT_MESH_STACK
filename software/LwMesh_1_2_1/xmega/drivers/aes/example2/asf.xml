<asf xmlversion="1.0">
	<module type="application" id="xmega.drivers.aes.example2" caption="AES Example 2">
		<info type="description" value="summary">
		Use the XMEGA built-in AES accelerator with interrupts. A
		single block is encrypted and decrypted using interrupts to
		control the process flow.
		</info>
		<info type="keyword" value="technology">
			<keyword value="Cryptography"/>
		</info>
		<build type="c-source" value="aes_example2.c"/>
		<build type="include-path" value="."/>
		<device-support value="xmegaa"/>
		<device-support value="xmegaau"/>
		<device-support value="xmegac"/>
		<require idref="xmega.drivers.aes"/>
		<require idref="xmega.drivers.pmic"/>
		<require idref="xmega.drivers.ioport"/>
		<require idref="common.services.basic.sleepmgr"/>
		<require idref="common.services.basic.clock"/>
		<info type="gui-flag" value="move-to-root"/>
	</module>
</asf>
