<asf xmlversion="1.0">
	<select-by-device id="sam.drivers.rswdt.unit_tests" caption="RSWDT Unit Tests">
		<module type="application" id="sam.drivers.rswdt.unit_tests#sam" caption="Unit Tests for the SAM RSWDT Driver">
			<info type="description" value="summary">
				Unit test application for RSWDT driver.
			</info>
			<build type="c-source" value="unit_tests.c"/>
			<build type="module-config" subtype="required-header-file" value="conf_test.h"/>
			<build type="include-path" value="."/>
			<device-support value="sam4e"/>
			<require idref="sam.drivers.pio"/>
			<require idref="sam.drivers.pmc"/>
			<require idref="sam.drivers.rswdt"/>
			<require idref="common.utils.unit_test"/>
			<require idref="common.utils.stdio.stdio_serial"/>
			<info type="gui-flag" value="move-to-root"/>
		</module>
	</select-by-device>
</asf>
