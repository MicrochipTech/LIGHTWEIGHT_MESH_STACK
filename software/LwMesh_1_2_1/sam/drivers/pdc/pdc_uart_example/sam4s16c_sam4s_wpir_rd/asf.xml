<asf xmlversion="1.0">
	<project caption="Peripheral DMA Controller example for SAM4S-WPIR-RD" id="sam.drivers.pdc.pdc_uart_example.sam4s16c_sam4s_wpir_rd">
		<!-- Summary description -->
		<info type="description" value="summary">
			This example shows how to configure PDC for receiving data from UART.
		</info>

		<!-- Files -->
		<build type="include-path" value="."/>
		<build type="header-file" value="conf_board.h"/>
		<build type="header-file" value="conf_clock.h"/>
		<build type="header-file" value="conf_pdc_uart_example.h"/>
		<build type="header-file" value="conf_uart_serial.h"/>

		<!-- Device support -->
		<device-support value="atsam4s16c"/>

		<!-- Required modules -->
		<require idref="board.sam4s_wpir_rd"/>
		<require idref="sam.drivers.pdc.pdc_uart_example"/>

		<!-- Generator options -->
		<generator value="iarewarm"/>
		<generator value="armgcc"/>
		<generator value="as5_arm"/>
		<config name="config.compiler.iarewarm.create_aux" value="no"/>
		<config name="config.compiler.armgcc.create_aux" value="no"/>
		<config name="config.compiler.as5_arm.create_aux" value="no"/>
	</project>
</asf>
