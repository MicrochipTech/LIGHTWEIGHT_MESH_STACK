<asf xmlversion="1.0">
	<select-by-device id="sam.drivers.emac.emac_example" caption="EMAC Example">
		<module type="application" id="sam.drivers.emac.emac_example#0" caption="EMAC Example">
			<info type="description" value="summary">
			This example will demonstrates how to configure and use the EMAC peripheral.
			</info>
			<build type="c-source" value="emac_example.c"/>
			<build type="include-path" value="."/>
			<build type="header-file" value="mini_ip.h"/>
			<device-support value="sam3x"/>
			<require idref="sam.utils.cmsis.sam3x.source.template"/>
			<require idref="sam.components.ethernet_phy.dm9161a"/>
			<require idref="sam.drivers.emac"/>
			<require idref="sam.drivers.pio"/>
			<require idref="sam.drivers.pmc"/>
			<require idref="sam.drivers.rstc"/>
			<require idref="sam.drivers.uart"/>
			<require idref="sam.utils.syscalls"/>
			<require idref="common.services.basic.gpio"/>
			<require idref="common.services.basic.clock"/>
			<require idref="common.utils.stdio.stdio_serial"/>
			<info type="gui-flag" value="move-to-root"/>
		</module>
	</select-by-device>
</asf>
